package helloworld;

public class Player {
int number = 0; // where the guess goes
public void guess() {
number = (int) (Math.random() * 10);
System.out.println("I’m guessing"+ number);
}
}
public class GameLauncher {
public static void main (String[] args) {
GuessGame game = new GuessGame();
game.startGame();
}
}